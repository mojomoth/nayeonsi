{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
      "react/prop-types": "off",
      "global-require": "off",
      "react/prefer-stateless-function": "off",
      "import/no-extraneous-dependencies": "off",
      "import/extensions": "off",
      "no-trailing-spaces": "off"
  }
}